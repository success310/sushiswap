{
  "name": "@sushiswap/cli",
  "version": "0.1.3",
  "description": "Sushi CLI",
  "keywords": [
    "sushi",
    "cli"
  ],
  "license": "MIT",
  "author": "Matthew Lilley <hello@matthewLilley.com>",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "bin": {
    "scli": "./dist/index.js"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup index.ts --format esm,cjs --watch --dts",
    "lint": "TIMING=1 eslint ./src --fix",
    "start": "ts-node --transpileOnly index.ts",
    "test": "jest --passWithNoTests"
  },
  "jest": {
    "preset": "@sushiswap/jest-config/node"
  },
  "dependencies": {
    "@sushiswap/graph-client": "workspace:*",
    "@sushiswap/graph-config": "workspace:*",
    "@sushiswap/format": "workspace:*",
    "@sushiswap/chain": "workspace:*",
    "@sushiswap/currency": "workspace:*",
    "@sushiswap/log": "workspace:*",
    "graphql": "@sushiswap/graph-client@workspace:*"
  },
  "devDependencies": {
    "@sushiswap/eslint-config": "workspace:*",
    "@sushiswap/jest-config": "workspace:*",
    "@sushiswap/prettier-config": "workspace:*",
    "@sushiswap/typescript-config": "workspace:*",
    "@types/node": "^17.0.31",
    "@types/numeral": "^2.0.2",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.0",
    "commander": "^9.4.0",
    "date-fns": "^2.29.0",
    "eslint": "^8.20.0",
    "jest": "^28.1.0",
    "numeral": "^2.0.0",
    "ora": "^5.4.1",
    "ts-node": "^10.9.0",
    "tsup": "^6.2.0",
    "typescript": "4.8.2"
  }
}
